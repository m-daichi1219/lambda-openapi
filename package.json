{
  "name": "lambda-openapi",
  "version": "0.1.0",
  "description": "TypeScript decorators to automatically generate OpenAPI documentation for AWS Lambda handlers",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "lambda-openapi": "lib/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test",
    "dev": "ts-node src/cli.ts",
    "docs:build": "typedoc src/index.ts --out docs/api",
    "example": "ts-node examples/basic/generate.ts"
  },
  "keywords": [
    "aws",
    "lambda",
    "openapi",
    "swagger",
    "typescript",
    "decorators",
    "api-gateway",
    "serverless",
    "documentation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/lambda-openapi.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/lambda-openapi/issues"
  },
  "homepage": "https://github.com/yourusername/lambda-openapi#readme",
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "typescript": "^5.0.0",
    "yargs": "^17.7.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.24.0"
  },
  "peerDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
